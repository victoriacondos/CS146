/*
 * Victoria Condos
 */
public class GenQueue<T> {
	private class ListNode
	{
		private T data;
		private ListNode link;
		public ListNode(T aData, ListNode aLink)
		{
			data=aData;
			link=aLink;
		}
	}
	private ListNode head;
	private ListNode tail;
	private int size;
	public GenQueue()
	{
		head=tail=null;
		size=0;
	}
	public void enqueue(T aData)//insert method
	{
		ListNode newNode= new ListNode(aData, null);
		if(head==null)
		{
			head=tail=newNode;
			size=1;
					return;
		}
		tail.link=newNode;
		tail=newNode;
		size++;
	}
	public T dequeue()//only removes from head
	{
		if(head==null)
			return null;
		T ret=head.data;
		head=head.link;
		size--;
		return ret;
	}
	public T peek()
	{
		if(head==null)
			return null;
		return head.data;
	}
	public void print()
	{
		for(ListNode temp=head; temp!=null; temp=temp.link)
			System.out.println(temp.data);
	}
	public int size()
	{
		
		return this.size;
	}	

}
